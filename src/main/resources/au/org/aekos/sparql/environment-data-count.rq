PREFIX api: <http://www.aekos.org.au/api/1.0#>

SELECT (COUNT(DISTINCT ?s) as ?count)
WHERE {
     ?s a api:location .
     ?s api:locationID ?locationID .
     VALUES ?locationID { %LOCATION_ID_PLACEHOLDER% } .
#OFF ?s api:disturbanceEvidenceVars | api:landscapeVars | api:noUnitVars | api:rainfallVars | api:soilVars | api:temperatureVars | api:windVars ?envVar .
#OFF ?envVar api:name ?varName .
#OFF VALUES ?varName { %ENV_VAR_PLACEHOLDER% } .
}
